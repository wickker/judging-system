{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 2,
	"bracketSpacing": true,
	"semi": false,
	"printWidth": 100,
	"jsxSingleQuote": true,
	"arrowParens": "always",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
